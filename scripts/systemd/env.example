# MCP Shell Service Environment
# Copy to /etc/mcp-persistent-shell/env and customize

MCP_SHELL_HOST=127.0.0.1
MCP_SHELL_PORT=3000
MCP_SHELL_CONFIG_FILE=/etc/mcp-persistent-shell/security.yaml
MCP_SHELL_LOG_LEVEL=info
MCP_SHELL_LOG_FORMAT=json
MCP_SHELL_SESSION_TIMEOUT=3600
MCP_SHELL_SESSION_MAX=100
